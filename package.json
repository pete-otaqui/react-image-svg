{
  "name": "react-svg-image",
  "version": "1.0.0-alpha.0",
  "description": "React-compatible conversion of SVG to images",
  "main": "dist/index.js",
  "type": "module",
  "repository": "https://github.com/pete-otaqui/react-svg-image",
  "author": "Pete Otaqui <pete@otaqui.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:src && yarn build:compress && yarn build:examples",
    "build:compress": "esbuild dist/index.js --bundle --minify --outfile=dist/index.min.js",
    "build:examples": "cp dist/index.js examples/react-16/react-svg-image.js && cp dist/index.js examples/react-18/react-svg-image.js",
    "build:src": "tsc -p tsconfig.json",
    "examples": "http-server examples"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x",
    "react-dom": "^16.x || ^17.x || ^18.x"
  },
  "devDependencies": {
    "@types/react": "^16.x || ^17.x || ^18.x",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "esbuild": "^0.15.12",
    "eslint": "^8.25.0",
    "http-server": "^14.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
